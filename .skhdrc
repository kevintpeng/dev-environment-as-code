# focus window
ctrl - h : yabai -m window --focus west
ctrl - j : yabai -m window --focus south
ctrl - k : yabai -m window --focus north
ctrl - l : yabai -m window --focus east

# move window
ctrl + cmd - h : yabai -m window --warp west
ctrl + cmd - j : yabai -m window --warp south
ctrl + cmd - k : yabai -m window --warp north
ctrl + cmd - l : yabai -m window --warp east

# fast focus desktop
ctrl - tab : yabai -m space --focus recent && say "$(( $(yabai -m query --spaces --space | jq '.index') - 1 ))"
ctrl - 0 : yabai -m space --focus 1 && say 0
ctrl - 1 : yabai -m space --focus 2 && say 1
ctrl - 2 : yabai -m space --focus 3 && say 2
ctrl - 3 : yabai -m space --focus 4 && say 3
ctrl - 4 : yabai -m space --focus 5 && say 4
ctrl - 5 : yabai -m space --focus 6 && say 5
ctrl - 6 : yabai -m space --focus 7 && say 6
ctrl - 7 : yabai -m space --focus 8 && say 7

# send window to desktop
ctrl + cmd - tab : yabai -m window --space recent; yabai -m space --focus recent && say "$(( $(yabai -m query --spaces --space | jq '.index') - 1 ))"
ctrl + cmd - 0 : yabai -m window --space 1 && yabai -m space --focus 1 && say 0
ctrl + cmd - 1 : yabai -m window --space 2 && yabai -m space --focus 2 && say 1
ctrl + cmd - 2 : yabai -m window --space 3; yabai -m space --focus 3 && say 2
ctrl + cmd - 3 : yabai -m window --space 4; yabai -m space --focus 4 && say 3
ctrl + cmd - 4 : yabai -m window --space 5; yabai -m space --focus 5 && say 4
ctrl + cmd - 5 : yabai -m window --space 6; yabai -m space --focus 6 && say 5
ctrl + cmd - 6 : yabai -m window --space 7; yabai -m space --focus 7 && say 6
ctrl + cmd - 7 : yabai -m window --space 8; yabai -m space --focus 8 && say 7
ctrl + cmd - w : yabai -m space --destroy && say 0 # assume sends back to main
ctrl + cmd - n : yabai -m space --create && \
                  index="$(yabai -m query --spaces --display | jq 'map(select(."native-fullscreen" == 0))[-1].index')" && \
                  yabai -m window --space "${index}" && \
                  yabai -m space --focus "${index}" && say "$(( ${index} - 1 ))"

ctrl + shift - h : yabai -m window --resize left:-100:0; yabai -m window --resize right:-100:0
ctrl + shift - j : yabai -m window --resize top:0:100; yabai -m window --resize bottom:0:100
ctrl + shift - k : yabai -m window --resize top:0:-100; yabai -m window --resize bottom:0:-100
ctrl + shift - h : yabai -m window --resize left:-100:0; yabai -m window --resize right:-100:0
ctrl + shift - l : yabai -m window --resize left:100:0; yabai -m window --resize right:100:0

# rotate tree
ctrl + cmd - r : yabai -m space --rotate 90

